<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:macro name="cx110l_description">

  <!-- helper macro -->
  <xacro:include filename="$(find khi2cpp_hw_description)/cx110l/urdf/inc/create_link.xacro"/>

  <xacro:create_link
      link_name="base_link"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="khi2cpp_hw_description/cx110l"
      mesh_name="cx165l-base"
  />

  <xacro:create_link
      link_name="link_1"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="khi2cpp_hw_description/cx110l"
      mesh_name="cx165l-j1"
  />

  <xacro:create_link
      link_name="link_2"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="khi2cpp_hw_description/cx110l"
      mesh_name="cx165l-j2"
  />

  <xacro:create_link
      link_name="link_3"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="khi2cpp_hw_description/cx110l"
      mesh_name="cx165l-j3"
  />

  <xacro:create_link
      link_name="link_4"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="khi2cpp_hw_description/cx110l"
      mesh_name="cx165l-j4"
  />

  <xacro:create_link
      link_name="link_5"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="khi2cpp_hw_description/cx110l"
      mesh_name="cx165l-j5"
  />

  <xacro:create_link
      link_name="link_6"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="khi2cpp_hw_description/cx110l"
      mesh_name="cx165l-j6"
  />

  <xacro:create_link
      link_name="pushcorp"
      xyz_offset="0 0 0"
      rpy_offset="0 0 0"
      mesh_package="khi2cpp_hw_description/cx110l"
      mesh_name="cx165l-pushcorp"
  />

  <link name="tool0"/>

  <!-- base_joint fixes base_link to the environment -->
  <joint name="base_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 -1"/>
  </joint>

  <!-- joints - main serial chain -->
  <joint name="joint_1" type="revolute">
    <parent link="base_link"/>
    <child link="link_1"/>
    <origin xyz="0 0 0.55" rpy="0 0 0"/>
    <axis xyz="0 0 -1"/>
    <limit effort="1000.0" lower="${-pi}" upper="${pi}" velocity="2.5"/>
  </joint>

  <joint name="joint_2" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin xyz="0 0.3 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit effort="1000.0" lower="${-pi}" upper="${pi}" velocity="2.5"/>
  </joint>

  <joint name="joint_3" type="revolute">
    <parent link="link_2"/>
    <child link="link_3"/>
    <origin xyz="0 0 1.19" rpy="0 0 0" />
    <axis xyz="1 0 0"/>
    <limit effort="1000.0" lower="${-pi}" upper="${pi}" velocity="2.5"/>
  </joint>

  <joint name="joint_4" type="revolute">
    <parent link="link_3"/>
    <child link="link_4"/>
    <origin xyz="0 1.19 0.3" rpy="0 0 0" />
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="${-pi}" upper="${pi}" velocity="2.5"/>
  </joint>

  <joint name="joint_5" type="revolute">
    <parent link="link_4"/>
    <child link="link_5"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit effort="1000.0" lower="${-pi}" upper="${pi}" velocity="2.5"/>
  </joint>

  <joint name="joint_6" type="revolute">
    <parent link="link_5"/>
    <child link="link_6"/>
    <origin xyz="0 0.235 0" rpy="0 0 0" />
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="${-pi}" upper="${pi}" velocity="2.5"/>
  </joint>

  <joint name="pushcorp" type="fixed">
    <parent link="link_6"/>
    <child link="pushcorp"/>
      <origin xyz="0 0.0949325000000025 0" rpy="0 1.5707963267949 0" />
  </joint>

  <joint name="joint-tool" type="fixed">
    <parent link="pushcorp"/>
    <child link="tool0"/>
    <origin xyz="0 0 .185" rpy="0 0 0"/>
  </joint>

</xacro:macro>

</robot>
